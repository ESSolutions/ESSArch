	"specification" : {	
		"-name": "mets",
		"-namespace": "mets",
		"-nsmap": {
			"mets": "http://www.loc.gov/METS/",
			"xsi": "http://www.w3.org/2001/XMLSchema-instance",
			"xlink": "http://www.w3.org/1999/xlink",
			"csip": "https://DILCIS.eu/XML/METS/CSIPExtensionMETS"
		},
		"-schemasToPreserve": [
			"http://www.loc.gov/standards/mets/mets.xsd",
			"http://www.loc.gov/standards/mets/xlink.xsd",
			"http://www.loc.gov/standards/premis/premis.xsd",
			"https://earkcsip.dilcis.eu/schema/DILCISExtensionMETS.xsd"
		],
		"-schemaPreservationLocation": "xsd_files",
		"-attr": [
			{
				"-name": "schemaLocation",
				"-namespace": "xsi",
				"#content": [{"text": "http://www.loc.gov/METS/ http://www.loc.gov/standards/mets/mets.xsd https://DILCIS.eu/XML/METS/CSIPExtensionMETS https://earkcsip.dilcis.eu/schema/DILCISExtensionMETS.xsd"}]
			},
			{
				"-name": "PROFILE",
				"-req": true,
				"#content": [{"text": "https://earkcsip.dilcis.eu/profile/E-ARK-CSIP.xml"}]
			},
			{
				"-name": "ID",
				"-req": false,
				"#content": [{"text": "ID"},{"var": "_UUID"}]
			},
			{
				"-name": "OBJID",
				"-req": true,
				"#content": [{"text": "UUID:"},{"var": "_OBJID"}]
			},
			{
				"-name": "LABEL",
				"-req": false,
				"#content": [{"var": "_OBJLABEL"}]
			},
			{
				"-name": "TYPE",
				"-req": true,
				"#content": "{{SIP_TYPE}}"
			},
			{
				"-name": "OTHERTYPE",
				"-namespace": "csip",
				"-req": false,
				"#content": "{{SIP_OTHERTYPE}}"
			},
			{
				"-name": "CONTENTINFORMATIONTYPE",
				"-namespace": "csip",
				"-req": false,
				"#content": "{{SIP_CONTENTINFORMATIONTYPE}}"
			},
			{
				"-name": "OTHERCONTENTINFORMATIONTYPE",
				"-namespace": "csip",
				"-req": false,
				"#content": "{{SIP_OTHERCONTENTINFORMATIONTYPE}}"
			}
		],
		"-children": [
			{
				"-name": "metsHdr",
				"-namespace": "mets",
				"-req": true,
				"-attr": [
					{
						"-name": "ID",
						"-req": false,
						"#content": [{"text": "ID"},{"var": "_UUID"}]
					},
					{
						"-name": "CREATEDATE",
						"-req": true,
						"#content": [{"var": "_NOW"}]
					},
					{
						"-name": "LASTMODDATE",
						"-req": false,
						"#content": [{"var": "mets.metsHdr.LASTMODDATE"}]
					},
					{
						"-name": "OAISPACKAGETYPE",
						"-namespace": "csip",
						"-req": true,
						"#content": [{"text": "AIP"}]
					}
				],
				"-children": [
					{
						"-name": "agent",
						"-namespace": "mets",
						"-req": true,
						"-attr": [
							{
								"-name": "ROLE",
								"-req": true,
								"#content": [{"text":"CREATOR"}]
							},
							{
								"-name": "TYPE",
								"-req": true,
								"#content": [{"text":"OTHER"}]
							},
							{
								"-name": "OTHERTYPE",
								"-req": true,
								"#content": [{"text":"SOFTWARE"}]
							}
						],
						"-children": [
							{
								"-name": "name",
								"-namespace": "mets",
								"-req": true,
								"#content": "{{SIP_AGENTS.CREATOR_SOFTWARE.name}}"
							},
							{
								"-name": "note",
								"-namespace": "mets",
								"-req": true,
								"-attr": [{
									"-name": "NOTETYPE",
									"-namespace": "csip",
									"-req": true,
									"#content": "SOFTWARE VERSION"
								}],
								"#content": "{{SIP_AGENTS.CREATOR_SOFTWARE.notes.0}}"
							}
						]
					}
				]
			},
			{
				"-name": "dmdSec",
				"-namespace": "mets",
				"-req": false,
				"-containsFiles": true,
				"-filters": {"href": "(^metadata/descriptive/ead.xml)"},
				"-attr": [
					{
						"-name": "ID",
						"-req": true,
						"#content": [{"text": "ID"},{"var": "_UUID"}]
					},
					{
						"-name": "CREATED",
						"-req": true,
						"#content": [{"var": "_NOW"}]
					},
					{
						"-name": "STATUS",
						"-req": false,
						"#content": [{"var": "dmdSecStatus"}]
					}
				],
				"-children": [
					{
						"-name": "mdRef",
						"-namespace": "mets",
						"-req": false,
						"-attr": [
							{
								"-name": "LOCTYPE",
								"-req": true,
								"#content": [{"var": "FLoctype"}]
							},
							{
								"-name": "type",
								"-namespace": "xlink",
								"-req": true,
								"#content": [{"text": "simple"}]
							},
							{
								"-name": "href",
								"-namespace": "xlink",
								"-req": true,
								"#content": [{"text": "file:///"},{"var": "href"}]
							},
							{
								"-name": "MDTYPE",
								"-req": true,
								"#content": [{"text": "EAD"}]
							},
							{
								"-name": "MIMETYPE",
								"-req": true,
								"#content": [{"var": "FMimetype"}]
							},
							{
								"-name": "SIZE",
								"-req": true,
								"#content": [{"var": "FSize"}]
							},
							{
								"-name": "CREATED",
								"-req": true,
								"#content": [{"var": "FCreated"}]
							},
							{
								"-name": "CHECKSUM",
								"-req": true,
								"#content": [{"var": "FChecksum"}]
							},
							{
								"-name": "CHECKSUMTYPE",
								"-req": true,
								"#content": [{"var": "FChecksumType"}]
							}
						]
					}
				]
			},
			{
				"-name": "dmdSec",
				"-namespace": "mets",
				"-req": false,
				"-containsFiles": true,
				"-filters": {"href": "(^metadata/descriptive/eac.xml)"},
				"-attr": [
					{
						"-name": "ID",
						"-req": true,
						"#content": [{"text": "ID"},{"var": "_UUID"}]
					},
					{
						"-name": "CREATED",
						"-req": true,
						"#content": [{"var": "_NOW"}]
					},
					{
						"-name": "STATUS",
