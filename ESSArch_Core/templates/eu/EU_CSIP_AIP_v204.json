						"-namespace": "mets",
						"-req": false,
						"-attr": [
							{
								"-name": "LOCTYPE",
								"-req": true,
								"#content": [{"var": "FLoctype"}]
							},
							{
								"-name": "type",
								"-namespace": "xlink",
								"-req": true,
								"#content": [{"text": "simple"}]
							},
							{
								"-name": "href",
								"-namespace": "xlink",
								"-req": true,
								"#content": [{"text": "file:///"},{"var": "href"}]
							},
							{
								"-name": "MDTYPE",
								"-req": true,
								"#content": [{"text": "EAC-CPF"}]
							},
							{
								"-name": "MIMETYPE",
								"-req": true,
								"#content": [{"var": "FMimetype"}]
							},
							{
								"-name": "SIZE",
								"-req": true,
								"#content": [{"var": "FSize"}]
							},
							{
								"-name": "CREATED",
								"-req": true,
								"#content": [{"var": "FCreated"}]
							},
							{
								"-name": "CHECKSUM",
								"-req": true,
								"#content": [{"var": "FChecksum"}]
							},
							{
								"-name": "CHECKSUMTYPE",
								"-req": true,
								"#content": [{"var": "FChecksumType"}]
							}
						]
					}
				]
			},
			{
				"-name": "amdSec",
				"-namespace": "mets",
				"-req": false,
				"-skipIfNoChildren": true,
				"-attr": [],
				"-children": [
					{
						"-name": "digiprovMD",
						"-namespace": "mets",
						"-req": false,
						"-skipIfNoChildren": true,
						"-attr": [
							{
								"-name": "ID",
								"-req": true,
								"#content": [{"text": "ID"},{"var": "_UUID"}]
							},
							{
								"-name": "STATUS",
								"-req": false,
								"#content": [{"var": "amdSecStatus"}]
