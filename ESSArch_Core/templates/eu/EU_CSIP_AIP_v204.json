										"#content": [{"var": "FSize"}]
									},
									{
										"-name": "CREATED",
										"-req": true,
										"#content": [{"var": "FCreated"}]
									},
									{
										"-name": "CHECKSUM",
										"-req": true,
										"#content": [{"var": "FChecksum"}]
									},
