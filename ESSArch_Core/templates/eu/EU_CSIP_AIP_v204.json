						],
						"-children": [
							{
								"-name": "mdRef",
								"-namespace": "mets",
								"-req": false,
								"-containsFiles": true,
								"-filters": {"href": "(^metadata/preservation/premis.xml)"},
								"-attr": [
									{
										"-name": "LOCTYPE",
