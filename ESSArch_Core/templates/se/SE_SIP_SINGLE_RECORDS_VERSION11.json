{
    "-name": "mets",
    "-namespace": "mets",
    "-nsmap": {
        "mets": "http://www.loc.gov/METS/",
        "xsi": "http://www.w3.org/2001/XMLSchema-instance",
        "xlink": "http://www.w3.org/1999/xlink",
        "ext": "ExtensionMETS"
    },
    "-schemasToPreserve": [
        "http://xml.essarch.org/METS/version11/ESSPackageMETS.xsd",
        "http://xml.ra.se/e-arkiv/METS/version11/CSPackageExtensionMETS.xsd",
        "http://xml.ra.se/e-arkiv/xlink/xlink.xsd"
    ],
    "-schemaPreservationLocation": "xsd_files",
    "-attr": [
        {
            "-name": "schemaLocation",
            "#content": [
                {
                    "text": "http://www.loc.gov/METS/ http://xml.essarch.org/METS/version11/ESSPackageMETS.xsd ExtensionMETS http://xml.ra.se/e-arkiv/METS/version11/CSPackageExtensionMETS.xsd"
                }
            ],
            "-namespace": "xsi"
        },
        {
            "-name": "ID",
            "#content": [{"text": "ID"}, {"var": "_UUID"}]
        },
        {
            "#content": [{"text": "UUID:"}, {"var": "_OBJID"}],
            "-name": "OBJID",
            "-req": 1
        },
        {
            "-name": "LABEL",
            "#content": [{"var": "_OBJLABEL"}]
        },
        {
            "-name": "TYPE",
            "#content": [{"var": "mets_type"}],
            "-req": 1
        },
        {
            "-name": "PROFILE",
            "#content": [{"text": "http://xml.ra.se/e-arkiv/METS/version11/CommonSpecificationSwedenPackageProfile.xml"}],
            "-req": 1
        },
        {
            "-name": "OAISTYPE",
            "-namespace": "ext",
            "#content": [{"text": "SIP"}],
            "-req": 1
        }
    ],
    "-children": [
        {
            "-name": "metsHdr",
            "-namespace": "mets",
            "-attr": [
                {
                    "-name": "ID",
                    "#content": [{"text": "ID"}, {"var": "_UUID"}]
                },
                {
                    "-name": "CREATEDATE",
                    "#content": [{"var": "_NOW"}],
                    "-req": 1
                },
                {
                    "-name": "RECORDSTATUS",
                    "#content": [{"var": "RECORDSTATUS"}]
                }
            ],
            "-children": [
                {
                    "-name": "agent",
                    "-namespace": "mets",
                    "-foreach": "_AGENTS",
                    "-attr": [
                        {
                            "-name": "ROLE",
                            "-req": 1,
                            "#content": [{"var": "_AGENTS_ROLE"}]
                        },
                        {
                            "-name": "TYPE",
                            "-req": 1,
                            "#content": [{"var": "_AGENTS_TYPE"}]
                        },
                        {
                            "-name": "OTHERROLE",
                            "#content": [{"var": "_AGENTS_OTHERROLE"}]
                        },
                        {
                            "-name": "OTHERTYPE",
                            "#content": [{"var": "_AGENTS_OTHERTYPE"}]
                        }
                    ],
                    "-children": [
                        {
                            "-name": "name",
                            "-namespace": "mets",
                            "#content": [{"var": "_AGENTS_NAME"}]
                        },
                        {
                            "-name": "note",
                            "-namespace": "mets",
                            "-foreach": "_AGENTS_NOTES",
                            "#content": [{"var": "_AGENTS_NOTE"}]
                        }
                    ]
                },
                {
                    "-name": "agent",
                    "-namespace": "mets",
                    "-attr": [{
                        "-name": "ROLE",
                        "#content": [{"text":"CREATOR"}]
                    },{
                        "-name": "TYPE",
                        "#content": [{"text":"ORGANIZATION"}]
                    }],
                    "-children": [
                        {
                            "-name": "name",
                            "-namespace": "mets",
                            "#content": [{"var":"creator_organization_name"}]
                        },
                        {
                            "-name": "note",
                            "-namespace": "mets",
                            "#content": [{"var":"creator_organization_note"}]
                        }
                    ]
                },{
                    "-name": "agent",
                    "-namespace": "mets",
                    "-attr": [{
                        "-name": "ROLE",
                        "#content": [{"text":"ARCHIVIST"}]
                    },{
                        "-name": "TYPE",
                        "#content": [{"text":"OTHER"}]
                    },{
                        "-name": "OTHERTYPE",
                        "#content": [{"text":"SOFTWARE"}]
                    }],
                    "-children": [
                        {
                            "-name": "name",
                            "-namespace": "mets",
                            "#content": [{"var":"archivist_software_name"}]
                        },
                        {
                            "-name": "note",
                            "-namespace": "mets",
                            "#content": [{"var":"archivist_software_note"}]
                        }
                    ]
                },
                {
                    "#content": [
                        {
                            "var": "_SA_ID"
                        }
                    ],
                    "-attr": [
                        {
                            "#content": [
                                {
                                    "text": "SUBMISSIONAGREEMENT"
                                }
                            ],
                            "-name": "TYPE"
                        }
                    ],
                    "-children": [],
                    "-name": "altRecordID",
                    "-namespace": "mets"
                },
                {
                    "#content": [{ "var": "_POLICYID"}],
                    "-attr": [{"-name": "TYPE", "#content": [{"text": "POLICYID"}]}],
                    "-name": "altRecordID",
                    "-namespace": "mets"
                },
                {
                    "#content": [{ "var": "_REFERENCECODE"}],
                    "-attr": [{"-name": "TYPE", "#content": [{"text": "REFERENCECODE"}]}],
                    "-name": "altRecordID",
                    "-namespace": "mets"
                },
                {
                    "#content": [{ "var": "_POLICYNAME"}],
                    "-attr": [{"-name": "TYPE", "#content": [{"text": "POLICYNAME"}]}],
                    "-name": "altRecordID",
                    "-namespace": "mets"
                },
                {
                    "-namespace": "mets",
                    "-name": "metsDocumentID",
                    "#content": [
                        {
                            "var": "_XML_FILENAME"
                        }
                    ]
                }
            ]
        },
        {
            "-name": "amdSec",
            "-namespace": "mets",
            "-hideEmptyContent": true,
            "-attr": [
                {
                    "-name": "ID",
                    "#content": [
                        {
                            "text": "ID"
                        },
                        {
                            "var": "_UUID"
                        }
                    ]
                }
            ],
            "-children": [
                {
                    "-name": "digiprovMD",
                    "-namespace": "mets",
                    "-hideEmptyContent": true,
                    "-attr": [
                        {
                            "-name": "ID",
                            "#content": [
                                {
                                    "text": "ID"
                                },
                                {
                                    "var": "_UUID"
                                }
                            ]
                        }
                    ],
                    "-children": [
                        {
                            "-name": "mdRef",
                            "-namespace": "mets",
                            "-containsFiles": true,
                            "-filters": {"href": "(metadata/premis.xml|metadata/administrative/premis.xml)"},
                            "-attr": [
                                {
                                    "-name": "MIMETYPE",
                                    "#content": [
                                        {
                                            "var": "FMimetype"
                                        }
                                    ]
                                },
                                {
                                    "-name": "href",
                                    "-namespace": "xlink",
                                    "#content": [{"text": "file:///"}, {"var": "href"}]
                                },
                                {
                                    "-name": "LOCTYPE",
                                    "#content": [{"var": "FLoctype"}]
                                },
                                {
                                    "-name": "CREATED",
                                    "#content": [{"var": "FCreated"}]
                                },
                                {
                                    "-name": "CHECKSUM",
                                    "#content": [{"var": "FChecksum"}]
                                },
                                {
                                    "-name": "type",
                                    "-namespace": "xlink",
                                    "#content": [{"text": "simple"}]
                                },
                                {
                                    "-name": "ID",
                                    "#content": [{"text": "ID"}, {"var": "FID"}]
                                },
                                {
                                    "-name": "MDTYPE",
                                    "#content": [{"text": "PREMIS"}]
                                },
                                {
                                    "-name": "CHECKSUMTYPE",
                                    "#content": [{"var": "FChecksumType"}]
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "-name": "fileSec",
            "-namespace": "mets",
            "-attr": [
                {
                    "-name": "ID",
                    "#content": [
                        {
                            "text": "ID"
                        },
                        {
                            "var": "_UUID"
                        }
                    ]
                }
            ],
            "-children": [
                {
                    "-name": "fileGrp",
                    "-namespace": "mets",
                    "-attr": [
                        {
                            "#content": [
                                {
                                    "text": "ID"
                                },
                                {
                                    "var": "_UUID"
                                }
                            ],
                            "-name": "ID"
                        }
                    ],
                    "-children": [
                        {
                            "-name": "file",
                            "-namespace": "mets",
                            "-containsFiles": true,
                            "-filters": {"href":"^(?!.*(metadata/premis.xml|metadata/administrative/premis.xml)).*$"},
                            "-attr": [
                                {
                                    "-name": "ID",
                                    "#content": [{"text": "ID"}, {"var": "FID"}],
                                    "-req": 1
                                },
                                {
                                    "-name": "MIMETYPE",
                                    "#content": [
                                        {
                                            "var": "FMimetype"
                                        }
                                    ],
                                    "-req": 1
                                },
                                {
                                    "-name": "SIZE",
                                    "#content": [
                                        {
                                            "var": "FSize"
                                        }
                                    ],
                                    "-req": 1
                                },
                                {
                                    "-name": "CREATED",
                                    "#content": [
                                        {
                                            "var": "FCreated"
                                        }
                                    ],
                                    "-req": 1
                                },
                                {
                                    "-name": "CHECKSUM",
                                    "#content": [
                                        {
                                            "var": "FChecksum"
                                        }
                                    ]
                                },
                                {
                                    "-name": "CHECKSUMTYPE",
                                    "#content": [
                                        {
                                            "var": "FChecksumType"
                                        }
                                    ]
                                }
                            ],
                            "-children": [
                                {
                                    "-name": "FLocat",
                                    "-namespace": "mets",
                                    "-attr": [
                                        {
                                            "-name": "LOCTYPE",
                                            "#content": [
                                                {
                                                    "text": "URL"
                                                }
                                            ]
                                        },
                                        {
                                            "-name": "href",
                                            "-namespace": "xlink",
                                            "#content": [
                                                {
                                                    "text": "file:///"
                                                },
                                                {
                                                    "var": "href"
                                                }
                                            ]
                                        },
                                        {
                                            "-name": "type",
                                            "-namespace": "xlink",
                                            "#content": [{"text": "simple"}]
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "-name": "structMap",
            "-namespace": "mets",
            "-attr": [
                {
                    "-name": "ID",
                    "#content": [
                        {
                            "text": "ID"
                        },
                        {
                            "var": "_UUID"
                        }
                    ]
                }
            ],
            "-children": [
                {
                    "-name": "div",
                    "-namespace": "mets",
                    "-attr": [
                        {
                            "-name": "LABEL",
                            "#content": [
                                {
                                    "var": "_OBJID"
                                }
                            ]
                        }
                    ],
                    "-children": [
                        {
                            "-name": "div",
                            "-namespace": "mets",
                            "-attr": [
                                {
                                    "-name": "LABEL",
                                    "#content": [
                                        {
                                            "text": "root"
                                        }
                                    ]
                                }
                            ],
                            "-children": [
                                {
                                    "-name": "fptr",
                                    "-namespace": "mets",
                                    "-containsFiles": true,
                                    "-filters": {"href":"^[^/]+$"},
                                    "-attr": [
                                        {
                                            "#content": [
                                                {
                                                    "text": "ID"
                                                },
                                                {
                                                    "var": "FID"
                                                }
                                            ],
                                            "-name": "FILEID",
                                            "-req": 1
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "-name": "div",
                            "-namespace": "mets",
                            "-attr": [
                                {
                                    "-name": "LABEL",
                                    "#content": [
                                        {
                                            "text": "content"
                                        }
                                    ]
                                }
                            ],
                            "-children": [
                                {
                                    "-containsFiles": true,
                                    "-filters": {"href":"content/*"},
                                    "-attr": [
                                        {
                                            "#content": [
                                                {
                                                    "text": "ID"
                                                },
                                                {
                                                    "var": "FID"
                                                }
                                            ],
                                            "-name": "FILEID",
                                            "-req": 1
                                        }
                                    ],
                                    "-children": [],
                                    "-name": "fptr",
                                    "-namespace": "mets"
                                }
                            ]
                        },
                        {
                            "-name": "div",
                            "-namespace": "mets",
                            "-attr": [
                                {
                                    "-name": "LABEL",
                                    "#content": [
                                        {
                                            "text": "metadata"
                                        }
                                    ]
                                }
                            ],
                            "-children": [
                                {
                                    "-containsFiles": true,
                                    "-filters": {"href":"metadata/*"},
                                    "-attr": [
                                        {
                                            "#content": [
                                                {
                                                    "text": "ID"
                                                },
                                                {
                                                    "var": "FID"
                                                }
                                            ],
                                            "-name": "FILEID",
                                            "-req": 1
                                        }
                                    ],
                                    "-children": [],
                                    "-name": "fptr",
                                    "-namespace": "mets"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
